                                                <Trigger Property="Height" Value="Auto">
                                                    <Setter Property="MinHeight" Value="20"/>
                                                </Trigger>
                                            </ControlTemplate.Triggers>
                                        </ControlTemplate>
                                    </Grid.Resources>
                                    <TextBox x:Name="txtSNnext" Template="{StaticResource TextBoxBaseControlTemplate}"   Background="#FFF1D1"  IsReadOnly="True"  />
                                </Grid>
                            </Grid>
                            <Grid Grid.Row="6">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="2*"></ColumnDefinition>
                                    <ColumnDefinition Width="7*"></ColumnDefinition>
                                    <ColumnDefinition Width="1*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="0">
                                    <Label Name="lblSNEnd" FontWeight="Bold" FontSize="16" Margin="0 0 10 0" Foreground="#FFF" VerticalAlignment="Center"  HorizontalAlignment="Right">SN End :</Label>
                                </Grid>
