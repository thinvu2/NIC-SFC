                    </Border>
                    <Border  Grid.Column="1" Margin="5 5 5 5" BorderBrush="#fff" BorderThickness="1.5" CornerRadius="5" Padding="0 0 5 0">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="0.2*"></RowDefinition>
                                <RowDefinition Height="0.8*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid Grid.Row="0">
                                <Label VerticalAlignment="Center" Background="#034E70" Foreground="White" FontStyle="Oblique" FontSize="24">SFIS Program :</Label>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="81*"/>
                                    <ColumnDefinition Width="244*"/>
                                </Grid.ColumnDefinitions>
                                <ListBox Name="RadioButtonList_Program" Background="#034E70" Foreground="Gray" Grid.ColumnSpan="2">
                                    <ListBox.Resources>
                                        <Style x:Key="{x:Type ListBoxItem}" TargetType="ListBoxItem">
                                            <Setter Property="SnapsToDevicePixels" Value="true"/>
                                            <Setter Property="OverridesDefaultStyle" Value="true"/>
                                            <Setter Property="Template">
                                                <Setter.Value>
                                                    <ControlTemplate TargetType="ListBoxItem">
                                                        <RadioButton x:Name="radio" Tag="{Binding ID}" IsEnabled="True" GroupName="groupPRG" Content="{Binding product_name}" Foreground="White" FontSize="20" Click="RadioPRGClick">
                                                        </RadioButton>
                                                    </ControlTemplate>
                                                </Setter.Value>
                                            </Setter>
                                        </Style>
                                    </ListBox.Resources>
                                </ListBox>

                            </Grid>
                        </Grid>
