                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Border Grid.Column="0" BorderBrush="White" BorderThickness="2" Height="40" CornerRadius="3" HorizontalAlignment="Center"  Margin="0,0,0,0" Width="150">
                        <Button x:Name="btnRelease" FontWeight="Bold" Content="Release" FontSize="26" Foreground="White" BorderBrush="White" BorderThickness="3" Cursor="Hand" IsEnabled="True" Click="btnRelease_Click">
                            <Button.Style>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="Background" Value="#034E70" />
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type Button}">
                                                <Border x:Name="Border" Background="{TemplateBinding Background}">
                                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                </Border>
                                                <ControlTemplate.Triggers>
                                                    <Trigger Property="IsMouseOver" Value="True">
                                                        <Setter Property="Background" Value="#0889A6" TargetName="Border" />
                                                    </Trigger>
                                                </ControlTemplate.Triggers>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </Button.Style>

                        </Button>
                    </Border>
                    <Border Grid.Column="1" BorderBrush="White" BorderThickness="2" Height="40" CornerRadius="3" HorizontalAlignment="Center"  Margin="0,0,0,0" Width="150">
                        <Button x:Name="btnPrint" FontWeight="Bold" Content="Print" FontSize="26" Foreground="White" BorderBrush="White" BorderThickness="3" Cursor="Hand" IsEnabled="True" Click="btnPrint_Click">
                            <Button.Style>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="Background" Value="#034E70" />
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type Button}">
                                                <Border x:Name="Border" Background="{TemplateBinding Background}">
                                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                </Border>
                                                <ControlTemplate.Triggers>
                                                    <Trigger Property="IsMouseOver" Value="True">
                                                        <Setter Property="Background" Value="#0889A6" TargetName="Border" />
                                                    </Trigger>
                                                </ControlTemplate.Triggers>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </Button.Style>
                        </Button>
                    </Border>


                    <Border Grid.Column="2" BorderBrush="White" BorderThickness="2" Height="40" CornerRadius="3" HorizontalAlignment="Center"  Margin="0,0,0,0" Width="150">
                        <Button x:Name="btnCheck" FontWeight="Bold" Content="Check" FontSize="26" Foreground="White" BorderBrush="White" BorderThickness="3" Cursor="Hand" IsEnabled="True" Click="btnCheck_Click">
                            <Button.Style>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="Background" Value="#034E70" />
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type Button}">
                                                <Border x:Name="Border" Background="{TemplateBinding Background}">
                                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                </Border>
                                                <ControlTemplate.Triggers>
                                                    <Trigger Property="IsMouseOver" Value="True">
                                                        <Setter Property="Background" Value="#0889A6" TargetName="Border" />
                                                    </Trigger>
                                                </ControlTemplate.Triggers>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </Button.Style>

                        </Button>
                    </Border>
                    <Border Grid.Column="3" BorderBrush="White" BorderThickness="2" Height="40" CornerRadius="3" HorizontalAlignment="Center"  Margin="0,0,0,0" Width="150">
                        <Button x:Name="btnVisible" FontWeight="Bold" Content="Visible" FontSize="26" Foreground="White" BorderBrush="White" BorderThickness="3" Cursor="Hand" IsEnabled="False" Click="btnVisible_Click">
                            <Button.Style>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="Background" Value="#034E70" />
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type Button}">
                                                <Border x:Name="Border" Background="{TemplateBinding Background}">
                                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                </Border>
                                                <ControlTemplate.Triggers>
                                                    <Trigger Property="IsMouseOver" Value="True">
                                                        <Setter Property="Background" Value="#0889A6" TargetName="Border" />
                                                    </Trigger>
                                                </ControlTemplate.Triggers>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </Button.Style>

                        </Button>
                    </Border>
                    <Border Grid.Column="4" BorderBrush="White" BorderThickness="2" Height="40" CornerRadius="3" HorizontalAlignment="Center"  Margin="0,0,0,0" Width="150">
                        <Button x:Name="btnShowParam" FontWeight="Bold" Content="ShowParam" FontSize="26" Foreground="White" BorderBrush="White" BorderThickness="3" Cursor="Hand" IsEnabled="False" Click="btnShowParam_Click">
                            <Button.Style>
                                <Style TargetType="{x:Type Button}">
                                    <Setter Property="Background" Value="#034E70" />
                                    <Setter Property="Template">
                                        <Setter.Value>
                                            <ControlTemplate TargetType="{x:Type Button}">
                                                <Border x:Name="Border" Background="{TemplateBinding Background}">
                                                    <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center" />
                                                </Border>
                                                <ControlTemplate.Triggers>
                                                    <Trigger Property="IsMouseOver" Value="True">
                                                        <Setter Property="Background" Value="#0889A6" TargetName="Border" />
                                                    </Trigger>
                                                </ControlTemplate.Triggers>
                                            </ControlTemplate>
                                        </Setter.Value>
                                    </Setter>
                                </Style>
                            </Button.Style>

                        </Button>
                    </Border>
                    <Border Grid.Column="5">
                        <Grid ShowGridLines="False">
                            <Grid.RowDefinitions>
                                <RowDefinition></RowDefinition>
                                <RowDefinition></RowDefinition>
                            </Grid.RowDefinitions>
                            <CheckBox Grid.Row="0" x:Name="chkPrintOver" IsEnabled="False" Margin="50 0 10 0"  Foreground="#FFF" Width="150" Height="40" FontWeight="Bold" VerticalContentAlignment="Center" FontSize="20" Cursor="Hand" >Print Over</CheckBox>
                            <CheckBox Grid.Row="1" x:Name="chkReprint" IsEnabled="False" Margin="50 0 10 0"  Foreground="#FFF" Width="150" Height="40" FontWeight="Bold" VerticalContentAlignment="Center" FontSize="20" Cursor="Hand" Checked="chkReprint_Checked" Unchecked="chkReprint_Unchecked" >Re-Print</CheckBox>
                        </Grid>
                    </Border>
                </Grid>

            </Border>
            <Border  Grid.Row="2" Margin="5 1 5 1" BorderBrush="#fff" BorderThickness="1.5" CornerRadius="5">
                <DataGrid Name="GridMain" ItemsSource="{Binding}" Background="LightGray" RowBackground="LightYellow" 
                          AlternatingRowBackground="LightBlue" IsReadOnly="True" HorizontalScrollBarVisibility="Visible" 
                          VerticalScrollBarVisibility="Visible" AreRowDetailsFrozen="True"  FrozenColumnCount="2" CanUserResizeColumns="False" EnableColumnVirtualization="False">

                </DataGrid>
            </Border>
            <Border Grid.Row="3" Margin="5 1 5 1" BorderBrush="#fff" BorderThickness="1.5" CornerRadius="5">
                <Label Name="lblMsg" FontStyle="Normal" Content="Show result" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="Red" FontSize="40" Height="Auto" BorderThickness="3,0,0,0">
                    <Label.Resources>
                        <Style TargetType="TextBlock">
                            <Setter Property="TextWrapping" Value="Wrap" />
                        </Style>
                    </Label.Resources>
                </Label>
            </Border>
            <Border Grid.Row="4" Margin="5 1 5 1" BorderBrush="#fff" BorderThickness="1.5" CornerRadius="5">
                <Grid>
                    <Grid.Resources>
                        <Style x:Key="MaterialDesignLinearProgressBar" TargetType="{x:Type ProgressBar}">
                            <Setter Property="Foreground" Value="{DynamicResource PrimaryHueDarkBrush}" />
                            <Setter Property="Background" Value="{DynamicResource PrimaryHueLightBrush}" />
                            <Setter Property="BorderBrush" Value="{DynamicResource PrimaryHueLightBrush}" />
                            <Setter Property="BorderThickness" Value="1" />
                            <Setter Property="VerticalContentAlignment" Value="Center"></Setter>
                            <Setter Property="HorizontalContentAlignment" Value="Center"></Setter>
                        </Style>
                    </Grid.Resources>
                    <ProgressBar
                    Background="White"
                        Foreground="#034E70"
                    Minimum="0" Maximum="100"
                    x:Name="prLoading"
                    Style="{StaticResource MaterialDesignLinearProgressBar}"                    
                    Height="30" />
                    <Label x:Name="lblLoading" HorizontalAlignment="Center" Foreground="Black" VerticalAlignment="Center" FontSize="15" >0 %</Label>

                </Grid>

            </Border>
        </Grid>
    </Border>
</UserControl>
