                                    <TextBox x:Name="txtLabelMode"  Template="{StaticResource TextBoxBaseControlTemplate}" Padding="5" Background="White" IsReadOnly="True" TextChanged="txtLabelMode_TextChanged" />
                                </Grid>
                                <Grid Grid.Column="3">
                                    <Button x:Name="cmdUp" x:FieldModifier="private" FontSize="10" Padding="0,0,0,0" Content="▲" Click="cmdUp_Click" Width="15" Height="15" Margin="0,0,0,15" HorizontalAlignment="Right"  />
                                    <Button x:Name="cmdDown" x:FieldModifier="private" FontSize="10" Padding="0,0,0,0" Content="▼"    Click="cmdDown_Click" Width="15" Height="15" Margin="0,0,0,-15"  HorizontalAlignment="Right" />
                                </Grid>
                            </Grid>
                            <Grid Grid.Row="1">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="2*"></ColumnDefinition>
                                    <ColumnDefinition Width="2.5*"></ColumnDefinition>
                                    <ColumnDefinition Width="2*"></ColumnDefinition>
                                    <ColumnDefinition Width="2.5*"></ColumnDefinition>
                                    <ColumnDefinition Width="1*"></ColumnDefinition>
                                </Grid.ColumnDefinitions>
                                <Grid Grid.Column="0">
                                    <Label FontWeight="Bold" FontSize="16" Margin="0 0 10 0" Foreground="#FFF" VerticalAlignment="Center"  HorizontalAlignment="Right">Mo Qty :</Label>
                                </Grid>
                                <Grid Grid.Column="1">
                                    <Grid.Resources>
                                        <Style TargetType="TextBox">
