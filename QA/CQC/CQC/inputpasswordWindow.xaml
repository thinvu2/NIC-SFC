<Window x:Class="CQC.inputpasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CQC"
        mc:Ignorable="d"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        WindowStartupLocation="CenterScreen"
        TextElement.FontSize="16"
        ResizeMode="NoResize"
        Title="Confirm TA Version" Height="150" Width="300">
    <StackPanel>
        <Label FontSize="10" Foreground="Red" HorizontalAlignment="Center">TA version Error,Please call PQE confirm!</Label>
        <PasswordBox
            Margin="30 10"
            x:Name="Edit1"
            materialDesign:HintAssist.Hint="Input Password"
            Style="{StaticResource MaterialDesignFloatingHintPasswordBox}" KeyDown="Edit1_KeyDown" />
        <Label FontSize="10" HorizontalAlignment="Center">Please  Input Password  Confirm!</Label>
    </StackPanel>
</Window>
